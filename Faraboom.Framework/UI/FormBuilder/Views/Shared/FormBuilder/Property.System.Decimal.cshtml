@using Faraboom.Framework.UI.FormBuilder
@model PropertyViewModel
<input @Html.Raw(Model.Readonly?"readonly=\"readonly\"":"") @Html.Raw(Model.Disabled?"disabled=\"disabled\"":"") @Html.UnobtrusiveValidation(Model) class="form-control" id="@Model.Id" name="@Model.Name" size="30" type="text" value="@(Model.Value ?? "")" />
